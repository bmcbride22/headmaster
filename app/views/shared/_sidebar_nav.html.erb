        <%= render_svg 'icons/messages', styles: "mr-4 h-6 w-6 flex-shrink-0 text-violet-300" %>
        Messages
      <% end %>
    </nav>
  </div>
  <div class="flex-shrink-0 flex border-t-2 border-t-violet-500 p-4">
    <%= link_to '#', class: "flex-shrink-0 w-full group block" do %>
      <div class="flex items-center">
        <div class="ml-2 self-center">
          <p class="text-sm font-medium text-violet-300 mb-0 group-hover:text-violet-200">
            <%= current_user.name %>
          </p>
          <p class="text-xs font-medium text-violet-300 mb-0 group-hover:text-violet-200">
            View profile
          </p>
        </div>
      </div>
    <% end %>
    <%= button_to destroy_user_session_path, method: :delete, class: "text-xs font-light text-gray-400 group" do %>
      <%= render_svg 'icons/log-out', styles: "mr-2 h-4 w-4 flex-shrink-0 text-violet-300 group-hover:text-orange-500" %>
      <p class="text-violet-700 text-xs  group-hover:text-orange-500">Sign out</p>
    <% end %>
  </div>
</div>
