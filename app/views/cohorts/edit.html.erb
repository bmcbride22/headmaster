<%# Simple edit page for the cohort %>
<h1>Edit Instrument</h1>
<%= simple_form_for @cohort do |f| %>
  <%= render "shared/error_messages", resource: f.object %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :start_date, as: :date%>
  <%= f.input :end_date, as: :date%>
  <%= f.submit %>
<% end %>
